//pb 1 pag 59 SÄƒ se realizeze programul care calculeaza suma elementelor unei matrice dreptunghiulare de dimensiuni mxn.

#include <stdio.h>
#include <conio.h>
int main()
{
    int n, m, i, j, s, a[100][100];
    printf("Nr de linii= ");
    scanf("%d", &m);
    printf("Nr de coloane= ");
    scanf("%d", &n);
    for (i = 0; i < m; i++)
        for (j = 0; j < n; j++)
        {
            printf("a[%d][%d] = ", i, j);
            scanf("%d", &a[i][j]);
        }
    for (i = 0; i < m; i++)
    {
        for (j = 0; j < n; j++)
            printf(" %d ", a[i][j]);
        printf("\n");
    }
    s = 0;
    for (i = 0; i < m; i++)
        for (j = 0; j < n; j++)
            s += a[i][j];
    printf("\n Suma elementelor este= %d ", s);
    getch();
}
